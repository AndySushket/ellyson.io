    "delaunator": "5.0.0",
    "firebase": "^10.1.0",
    "jest": "^29.6.2",
    "jest-resolve": "^29.6.2",
    "jest-watch-typeahead": "^2.2.2",
    "jest-cli": "^29.6.2",
    "mdi-react": "^9.2.0",
    "prettier": "^3.0.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
